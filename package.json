{
  "name": "group15_phase2",
  "version": "1.0.0",
  "description": "Repository Inherited from: Sydney Chang, Sam Johnson, Harrison Smith, Doha Hafez, Jana Gamal\r Contributors: Areej Mirani, Ethan Hunt, Yingchen Jin, Varun Venkatesh, Ruth Sugiarto",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "dev": "webpack serve --config webpack.config.js",
    "test": "playwright test --coverage --project=chromium",
    "coverage": "nyc --reporter=text --reporter=html --reporter=lcov playwright test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@playwright/test": "^1.49.0",
    "@types/archiver": "^6.0.3",
    "@types/aws-lambda": "^8.10.146",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.1",
    "aws-sdk": "^2.1692.0",
    "babel-loader": "^9.2.1",
    "babel-plugin-istanbul": "^7.0.0",
    "mocha": "^11.0.1",
    "nyc": "^17.1.0",
    "playwright": "^1.49.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "nyc": {
    "extension": [".ts", ".tsx"],   
    "require": [
      "ts-node/register"
    ],
    "all": true,
    "check-coverage": true,
    "per-file": true,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "exclude": [
      "**/*.d.ts",
      "node_modules/**/*",
      "tests/**/*.ts"
    ],
    "include": [
      "frontend/src/**/*.ts",
      "frontend/src/**/*.tsx",
      "backend/src",
      "backend/src/*.ts",
      "backend/src/*.tsx"
    ],
    "report-dir": "./coverage"
  },
  "dependencies": {
    "@aws-sdk/client-codeartifact": "^3.699.0",
    "@aws-sdk/client-dynamodb": "^3.705.0",
    "archiver": "^7.0.1",
    "axios": "^1.7.9",
    "jsonwebtoken": "^9.0.2",
    "tar": "^7.4.3",
    "web-vitals": "^4.2.4",
    "yarn": "^1.22.22"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "istanbul",
      "transform-css"
    ]
  }
}
